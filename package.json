{
    "name": "mackerel-expression-graph-syntax",
    "publisher": "stefafafan",
    "displayName": "Mackerel Expression Graph Syntax Highlighting",
    "description": "(Unofficial) Syntax Highlighting for the Mackerel Expression Graphs Language.",
    "version": "0.0.1",
    "license": "MIT",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "mackerel",
        "expression graph",
        "syntax",
        "highlighting"
    ],
    "homepage": "https://github.com/stefafafan/vscode-mackerel-expression-graph-syntax",
    "contributes": {
        "languages": [{
            "id": "mackerel",
            "aliases": ["Mackerel", "mackerel"],
            "extensions": [".mackerel"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "mackerel",
            "scopeName": "source.mackerel",
            "path": "./syntaxes/mackerel.tmLanguage.json"
        }]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/stefafafan/vscode-mackerel-expression-graph-syntax"
    }
}